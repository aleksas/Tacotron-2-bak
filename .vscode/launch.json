            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debugpy: synthesize",
            "type": "debugpy",
            "request": "launch",
            "program": "synthesize.py",
            "args": ["--model=Tacotron", "--mode=eval", "--text_list=text_list.txt"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debugpy: preprocess Vladas",
            "type": "debugpy",
            "request": "launch",
            "program": "preprocess.py",
            "args": ["--dataset=liepa-multi", "--voice=Vladas", "--output=training_data_Vladas"],
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Debugpy: train Vladas",
            "type": "debugpy",
            "request": "launch",
            "program": "train.py",
            "args": ["--model=Tacotron-2", "--name=Vladas", "--tacotron_input=training_data_Regina/train.txt", "--input_dir=training_data_Vladas", "--output_dir=output_Vladas"],
            "console": "integratedTerminal",
            "justMyCode": true
        }
